        "@vue/tsconfig": "^0.8.1",
        "electron": "^39.1.0",
        "eslint": "^9.35.0",
        "eslint-config-vuetify": "^4.2.0",
        "npm-run-all2": "^8.0.4",
        "sass": "^1.94.1",
